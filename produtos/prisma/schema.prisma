// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produtos {
  id String @id @default(uuid())
  id_usuario String
  nome String
  quantidade Float
  preco Float

  @@map("produtos")
}